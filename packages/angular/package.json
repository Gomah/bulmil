    "rxjs": "7.5.2"
  },
  "devDependencies": {
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/compiler-cli": "14.2.7",
    "@angular/core": "14.2.7",
    "ng-packagr": "14.2.2",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "typescript": "4.8.4"
  }
}
