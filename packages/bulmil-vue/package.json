{
  "name": "bulmil-vue",
  "sideEffects": false,
  "version": "0.5.0",
  "author": "Gomah",
  "license": "MIT",
  "description": "Vue specific wrapper for bulmil",
  "repository": {
    "type": "git",
    "url": "https://github.com/gomah/bulmil.git"
  },
  "scripts": {
    "test": "jest __tests__/",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@types/jest": "^26.0.3",
    "jest": "^26.1.0",
    "rollup": "^2.18.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.3",
    "bulmil": "^0.5.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
