{
  "name": "bulmil-vue",
  "sideEffects": false,
  "version": "0.7.1",
  "author": "Gomah",
  "license": "MIT",
  "description": "Vue specific wrapper for bulmil",
  "repository": {
    "type": "git",
    "url": "https://github.com/gomah/bulmil.git"
  },
  "scripts": {
    "test": "jest __tests__/",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.3",
    "@types/jest": "^26.0.9",
    "jest": "^26.4.0",
    "rollup": "^2.23.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.3",
    "bulmil": "0.7.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
