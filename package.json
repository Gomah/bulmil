{
  "name": "bulmil",
  "version": "0.0.1",
  "repository": "git@github.com:gomah/bulmil.git",
  "author": "Gomah <hi@gomah.fr>",
  "license": "MIT",
  "files": [
    "bulmil.scss",
    "src/",
    "dist/",
    "css/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs.js",
  "es2017": "dist/esm/index.mjs.js",
  "jsnext:main": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "css/bulmil.css",
  "style": "css/bulmil.min.css",
  "scripts": {
    "build": "yarn build:clean && yarn build:scss && yarn build:autoprefix && yarn build:min && yarn build:components",
    "build:autoprefix": "postcss --use autoprefixer --map false --output css/bulmil.css css/bulmil.css",
    "build:min": "cleancss -o css/bulmil.min.css css/bulmil.css",
    "build:scss": "node-sass --output-style expanded --source-map true bulmil.scss css/bulmil.css",
    "build:docs": "stencil build --docs",
    "build:clean": "rimraf css",
    "build:components": "stencil build --prod",
    "build:storybook": "build-storybook -c .storybook -s www -o storybook/",
    "start": "stencil build --dev --watch --serve",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "node --max_old_space_size=4096 ./node_modules/.bin/stencil build --watch --recursive --serve",
    "storybook.run": "start-storybook -p 9006 -s www",
    "storybook": "npm-run-all --parallel watch storybook.run",
    "prepublishOnly": "yarn build",
    "test:lint:ts": "eslint --ext .ts,.tsx --ignore-path .gitignore src/core/",
    "test:lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "test:stencil:e2e": "stencil test --e2e",
    "test:stencil:e2e.watch": "stencil test --e2e --watchAll",
    "test:stencil:unit": "stencil test --spec",
    "test:stencil:unit:watch": "stencil test --spec --watch",
    "test:stencil:snapshots": "stencil test --e2e --screenshot",
    "test:stencil": "yarn test:stencil:unit && yarn test:stencil:e2e",
    "test": "yarn test:lint:ts && yarn test:lint:scss && yarn test:stencil"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@stencil/core": "^1.4.0",
    "@stencil/eslint-plugin": "^0.0.3",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.1",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-centered": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/html": "^5.2.1",
    "@storybook/theming": "^5.2.1",
    "@types/autoprefixer": "^9.6.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/puppeteer": "1.19.1",
    "@types/storybook__addon-knobs": "^5.0.4",
    "@types/storybook__html": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.0",
    "autoprefixer": "^9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "clean-css-cli": "^4.3.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "puppeteer": "1.19.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "standard-version": "^7.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.1",
    "webpack": "^4.40.2",
    "workbox-build": "^4.3.1"
  },
  "dependencies": {
    "bulma-scss": "^0.7.5-1"
  }
}
